    issn = {0219-7200},
    journal = {Journal of bioinformatics and computational biology},
    keywords = {abinitio, frameshift},
    month = jun,
    number = {3},
    pages = {535--551},
    pmid = {20556861},
    posted-date = {2012-06-25 14:46:58},
    priority = {2},
    title = {Genetack: frameshift identification in protein-coding sequences by the Viterbi algorithm.},
